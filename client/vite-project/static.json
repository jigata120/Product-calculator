{
    "root": "dist",
    "routes": [
      {
        "src": "/assets/*.css",
        "dest": "/assets/*.css",
        "headers": {
          "Content-Type": "text/css"
        }
      },
      {
        "src": "/assets/*.js",
        "dest": "/assets/*.js",
        "headers": {
          "Content-Type": "application/javascript"
        }
      },
      {
        "src": "/.*",
        "dest": "/index.html"
      }
    ]
  }
  